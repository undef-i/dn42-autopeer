# Email Service Configuration
RESEND_API_KEY=your_resend_api_key_here
EMAIL_FROM=Your Service <noreply@yourdomain.com>

# Flask Configuration
SECRET_KEY=your_secret_key_here
FLASK_ENV=development
FLASK_DEBUG=True
FLASK_PORT=5000

# Database Configuration
DATABASE_PATH=database.db

# WireGuard Configuration
WIREGUARD_PRIVATE_KEY=your_wireguard_private_key_here
WIREGUARD_SERVER_IPV4=172.22.159.100
WIREGUARD_SERVER_IPV6=fdef:fa9b:62b::1
WIREGUARD_SERVER_IPV6_LL=fe80::454

# Git Repository Configuration
DN42_REGISTRY_URL=https://git.dn42.dev/dn42/registry.git
GIT_USERNAME=your_git_username
GIT_TOKEN=your_git_token

# Network Configuration
ALLOWED_IPS=10.0.0.0/8,172.20.0.0/14,172.31.0.0/16,fd00::/8,fe80::/64

# Development Configuration
DEBUG_BYPASS_KEY=your_debug_bypass_key_here